{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name":"Python: Current File",
      "type":"python",
      "request":"launch",
      "program":"${file}",
      "console":"integratedTerminal",
      "preLaunchTask": "build"
    },
    {
      "name": "C++ Node",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/${input:package}/lib/${input:package}/${input:program}",
      "args": [],
      "preLaunchTask": "build",
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "VIO Node",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/vio/lib/vio/vio_node",
      "args": [],
      "preLaunchTask": "build",
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ],
  "inputs": [
    {
      "id": "package",
      "type": "promptString",
      "description": "Package name",
    },
    {
      "id": "program",
      "type": "promptString",
      "description": "Program name",
    },
    {
      "id": "launch",
      "type": "promptString",
      "description": "Launch File",
    },
    
  ]
}